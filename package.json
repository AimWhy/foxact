    "@sukka/listdir": "^0.2.0",
    "@swc/core": "^1.3.75",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
